package cleaning;

public class EndView {
	public static void print(String pickedName[]) {
		int count = pickedName.length;
		for(int i = 0; i < count; i++) {
			if(i == count - 1) {
				System.out.print("´Ô ÀÔ´Ï´Ù. ");
			}
			System.out.print(pickedName[i] + "´Ô, ");
		}
			
		System.out.println();
	}
}
