package test.exam;

public class Tv extends Elec implements ElecFunction{
	private int channel;
	
	Tv() {
		this(0);
	}
	Tv(int channel) {
		this(null, 0, channel);
	}
	Tv(String code, int cost, int channel) {
		super(code, cost);
		this.channel = channel;
	}
	
	@Override
	public void start() {
		System.out.println(getCode() + "제품 TV를 " + channel + "을 본다.");
		
	}
	@Override
	public void stop() {
		// TODO Auto-generated method stub
		
	}
	@Override
	public void display() {
		// TODO Auto-generated method stub
		
	}
	
	@Override
	public String toString() {
		return null;
	}
	
}
