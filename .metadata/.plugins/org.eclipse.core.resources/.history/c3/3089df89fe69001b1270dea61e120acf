package testt;

public class Test {
	public static void main(String[] args) {		
		int num[] = new int[10];
		for(int i = 0; i < 10; i++ ) {
			num[i] = 0;
		}
		
		for(int i = 0; i < 5000; i++ ) {
			int randNum = (int)(Math.random() * 10);
			num[randNum]++;
		}
		
		for(int i : num)
			System.out.println(i);
	}
}
